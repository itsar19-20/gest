<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.TestActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/buttonTestClear"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="clear" />

        <Button
            android:id="@+id/buttonTestGet"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="get" />

        <Button
            android:id="@+id/buttonTestGetSpecial"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="get spacial" />

        <Button
            android:id="@+id/buttonTestPost"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="post" />

        <Button
            android:id="@+id/buttonTestPostMaxi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="post maxi" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="alfa" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="bravo" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="charlie" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>